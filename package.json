{
  "name": "@sigiljs/seal",
  "version": "0.0.0",
  "description": "Lightweight, TypeScript-first runtime data validator with a fluent, chainable API",
  "main": "dist/seal.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "vite build -- --production",
    "test": "vitest run --reporter=verbose"
  },
  "engines": {
    "node": ">=24.0.2"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/seal.mjs",
      "require": "./dist/seal.js"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas.mjs",
      "require": "./dist/schemas.js"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    }
  },
  "keywords": [
    "router",
    "http",
    "typescript",
    "zero-dependencies",
    "fast-routing",
    "node"
  ],
  "author": "Kurai Foundation",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.15.18",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kurai-foundation/seal.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/kurai-foundation/seal/issues"
  },
  "homepage": "https://github.com/kurai-foundation/seal#readme"
}
